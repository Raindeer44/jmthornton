  "dependencies": {
    "instant.page": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "babel-minify": "^0.5.1",
    "fibers": "^4.0.1",
    "filefile": "^1.0.2",
    "http-server": "^0.12.3",
    "markdown": "^0.5.0",
    "purify-css": "^1.2.5",
    "sass": "^1.22.12",

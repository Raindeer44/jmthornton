{
  "name": "jmthornton.net",
  "version": "3.3.0",
  "description": "Personal website of Jade Thornton",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thornjad/jmthornton.git"
  },
  "author": "Jade Michael Thornton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thornjad/jmthornton/issues"
  },
  "homepage": "https://github.com/thornjad/jmthornton",
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "scripts": {
    "protect": "snyk protect --yes",
    "prepare": "npm run protect",
    "snyk": "snyk",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "npm run lint-js && npm run lint-css",
    "fix-js": "npm run lint-js --fix",
    "fix-css": "npm run lint-css --fix",
    "fix": "npm run fix-js && npm run fix-css",
    "flow": "flow",
    "flow:check": "flow check",
    "pretest": "npm run lint && npm run flow:check",
    "test": "tap --jobs-auto --coverage test/*.test.js",
    "posttest": "tap --coverage-report=clover",
    "check": "npm-check",
    "npmcheckversion": "node ./util/scripts/npmcheckversion.js",
    "start": "npm test && webpack --config util/webpack/webpack.dev.babel.js --color -p --progress && http-server",
    "start:unsafe": "webpack --config util/webpack/webpack.dev.babel.js --color -p --progress && http-server",
    "generate": "plop --plopfile util/generators/index.js",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "bundle": "npm run build",
    "build": "webpack --config util/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf ./build",
  },
  "pre-commit": "test",
  "babel": {
    "presets": [
      [
        "env"
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-inline-elements",
          "transform-react-constant-elements"
        ]
      }
    }
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "helmet": "^3.13.0",
    "node-fetch": "^2.1.2",
    "normalize.css": "^8.0.0",
    "mini.css": "^3.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "http-server": "^0.11.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.51",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-flow": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-2": "^7.0.0-beta.51",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "autoprefixer": "^8.6.3",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "browser-sync": "^2.24.4",
    "chokidar": "^2.0.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^5.0.0",
    "eslint-config-semistandard": "^12.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^1.1.11",
    "filefile": "^1.0.1",
    "flow-bin": "^0.74.0",
    "front-matter": "^2.3.0",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^7.2.0",
    "markdown-it": "^8.4.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.3",
    "npm-check": "^5.8.0",
    "null-loader": "^0.1.1",
    "opn-cli": "^3.1.0",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "postcss": "^6.0.22",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-nesting": "^6.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "react-dev-utils": "^5.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "snyk": "^1.54.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "svg-url-loader": "^2.3.2",
    "tap": "^12.0.1",
    "webpack": "^4.12.0",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-node-externals": "^1.7.2"
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx}": [
        "eslint --no-ignore --fix",
        "git add --force"
      ],
      "*.{css,less,styl,scss,sass,sss}": [
        "stylelint --fix",
        "git add --force"
      ]
    }
  }
}
